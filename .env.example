# SAM+ Environment Configuration
# Copy this file to .env and fill in your actual values

# ============================================
# SUPABASE CONFIGURATION
# ============================================
# Primary Supabase instance
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key

# Secondary Supabase instance (if needed)
SUPABASE_URL_NEW=https://your-new-project.supabase.co
SUPABASE_ANON_KEY_NEW=your-new-supabase-anon-key

# ============================================
# GOOGLE APPS SCRIPT APIs
# ============================================
# SAM API endpoint
API_SAM=https://script.google.com/macros/s/YOUR_SAM_SCRIPT_ID/exec

# FEM API endpoint
API_FEM=https://script.google.com/macros/s/YOUR_FEM_SCRIPT_ID/exec

# SOLPE API endpoint
API_SOLPE=https://script.google.com/macros/s/YOUR_SOLPE_SCRIPT_ID/exec

# CHATARRA API endpoint
API_CHATARRA=https://script.google.com/macros/s/YOUR_CHATARRA_SCRIPT_ID/exec

# Third Supabase instance (SAP data)
SUPABASE_URL_SAP=https://your-sap-project.supabase.co
SUPABASE_ANON_KEY_SAP=your-sap-supabase-anon-key

# Fourth Supabase instance (LCL data)
SUPABASE_URL_LCL=https://your-lcl-project.supabase.co
SUPABASE_ANON_KEY_LCL=your-lcl-supabase-anon-key
